<?xml version="1.0" encoding="utf-8"?>
<mx:VBox xmlns:mx="http://www.adobe.com/2006/mxml"
	xmlns:views="views.*" 
	xmlns:widget="widget.*"
	 width="100%" height="100%">
	<mx:Script>
		<![CDATA[
			import mx.collections.ArrayCollection;
			import com.esria.samples.dashboard.view.Pod;
		]]>
	</mx:Script>
	
	
	<views:Index width="100%" height="100%">
		<views:cans>
			<mx:Canvas id="canvas" width="100%" height="100%" backgroundColor="0xFFFFFF">
				<widget:FirstCharge id="firstCharge" width="100%" height="100%" windowState="{Pod.WINDOW_STATE_DEFAULT}" param2="{this}"/>
				<widget:FirstCharge2 id="firstCharge2" width="100%" height="100%" windowState="{Pod.WINDOW_STATE_DEFAULT}" />
				<widget:FirstCharge3 id="firstCharge3" width="100%" height="100%" windowState="{Pod.WINDOW_STATE_DEFAULT}" />
				<widget:FirstCharge4 id="firstCharge4" width="100%" height="100%" windowState="{Pod.WINDOW_STATE_DEFAULT}" param2="{this}"/>
				<widget:FirstCharge5 id="firstCharge5" width="100%" height="100%" windowState="{Pod.WINDOW_STATE_DEFAULT}"/>
				<widget:FirstCharge6 id="firstCharge6" width="100%" height="100%" windowState="{Pod.WINDOW_STATE_DEFAULT}"/>
			</mx:Canvas>
		</views:cans>
	</views:Index>
</mx:VBox>
