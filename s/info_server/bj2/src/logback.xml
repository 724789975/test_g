<configuration scan="true">
  <appender name="file" class="ch.qos.logback.core.rolling.RollingFileAppender">
    <file>${catalina.base}/logs/fc.log</file>
    <rollingPolicy class="ch.qos.logback.core.rolling.TimeBasedRollingPolicy">
    	<fileNamePattern>${catalina.base}/logs/fc.%d{yyyy-MM-dd}.log</fileNamePattern>
    </rollingPolicy>
    <encoder>
      <pattern>%-4relative, %d [%thread]|-%-5level %logger{35} - %msg%n</pattern>
       <outputPatternAsPresentationHeader>false</outputPatternAsPresentationHeader>
    </encoder>
  </appender>

  <appender name="exception" class="ch.qos.logback.core.rolling.RollingFileAppender">
    <filter class="ch.qos.logback.classic.filter.ThresholdFilter">
      <level>ERROR</level>
    </filter>
    <file>${catalina.base}/logs/fc_error.log</file>
        <rollingPolicy class="ch.qos.logback.core.rolling.TimeBasedRollingPolicy">
    	<fileNamePattern>${catalina.base}/logs/fc_error.%d{yyyy-MM-dd}.log</fileNamePattern>
    </rollingPolicy> 
    <encoder>
      <pattern>%-4relative, %d [%thread]|-%-5level %logger{35} - %msg%n</pattern>
       <outputPatternAsPresentationHeader>false</outputPatternAsPresentationHeader>
    </encoder>
  </appender>

