<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN" "http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="Partner">

	<resultMap id="pid" class="java.lang.Long">
		<result property="id" column="ID_B" jdbcType="NUMBER" />
	</resultMap>

	<select id="getPartnerPkId" resultMap="pid" parameterClass="java.util.Map">
		SELECT
			ID_B
		FROM PARTNER
		WHERE PARTNER_ID_V = #partnerId# AND STATUS_I = #status#
	</select>
	
</sqlMap>