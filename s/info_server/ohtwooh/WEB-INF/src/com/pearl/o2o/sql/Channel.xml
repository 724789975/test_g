<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMap      
    PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"      
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="Channel">
<typeAlias alias="Channel" type="com.pearl.o2o.pojo.Channel"/> 
<resultMap id="selectChannelResult" class="Channel">
		<result property="id" column="ID"/>
		<result property="name" column="NAME"/>
		<result property="serverId" column="SERVER_ID"/>
	</resultMap>
	<select id="select" parameterClass="java.lang.Integer" resultMap="selectChannelResult">
		select * from CHANNEL c where c.SERVER_ID=#value#;
	</select>
</sqlMap>