<?xml version="1.0" encoding="utf-8"?>
<mx:VBox xmlns:mx="http://www.adobe.com/2006/mxml"
	xmlns:views="views.*" 
	xmlns:widget="widget.*"
	 width="100%" height="100%">
	<mx:Script>
		<![CDATA[
			import mx.collections.ArrayCollection;
			import com.esria.samples.dashboard.view.Pod;
			[Bindable]
			private var  datas:ArrayCollection=new ArrayCollection();
		]]>
	</mx:Script>
	
	
	<views:Index width="100%" height="100%">
		<views:cans>
			<mx:Canvas id="canvas" width="100%" height="100%" backgroundColor="0xFFFFFF">
				<widget:DauPlayer id="dauPlayer" width="100%" height="100%" windowState="{Pod.WINDOW_STATE_DEFAULT}" param2="{this}" datas="{datas}"/>
				
			</mx:Canvas>
		</views:cans>
	</views:Index>
</mx:VBox>
