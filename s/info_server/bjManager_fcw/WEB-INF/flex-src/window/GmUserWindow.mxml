<?xml version="1.0" encoding="utf-8"?>
<mx:VBox xmlns:mx="http://www.adobe.com/2006/mxml" width="100%" height="100%" xmlns:view="view.*">
	<mx:Script>
		<![CDATA[
			[Bindable]
			private var model:O2oModel = O2oModel.getInstance();
		]]>
	</mx:Script>
	<mx:TabBar dataProvider="{gmUserVS}"/>
	<mx:ViewStack id="gmUserVS" width="100%" height="100%">
		<view:GmUserSetting label="修改密码"/>
		<view:GmLogManage label="GM日志查询" enabled="{model.PRIV_LOG}"/>
		<view:GmUserMange label="GM用户管理" enabled="{model.PRIV_USER}"/>
		<view:GmGroupMange label="GM群组管理" enabled="{model.PRIV_GROUP}"/>
	</mx:ViewStack>
</mx:VBox>
